	journal = {Physics of Life Reviews},
	author = {Wiggins, Geraint A.},
	year = {2020},
	keywords = {Cognitive architecture, Cognitive representation, Computational creativity, Information theory, Machine consciousness, Music cognition},
	pages = {1--39},
}

