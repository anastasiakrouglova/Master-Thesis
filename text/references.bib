@article{homer_modelling_2023,
	title = {Modelling of {Musical} {Perception} using {Spectral} {Knowledge} {Representation}},
	abstract = {We present a novel approach to representing low-level musical knowledge in a cognitive model that is focused on the oscillatory behaviour of the brain. The model uses Hilbert space representations of sound to model perception at the level of the Organ of Corti and then as percepts such as pitch, tonic and chord. We demonstrate that the model naturally captures both pitch and chord/key distance as empirically measured by Krumhansl and Kessler. We demonstrate that our method correctly predicts the missing fundamental phenomenon, and then, inspired by the harmonic theory of Rameau, that it is capable of predicting the perceived harmonic centre of a musical sequence. We explain how the method forms a basis for higher-level knowledge structures, both in music and elsewhere, allowing predictive coding. We suggest how abstraction of musical features may be achieved in the mathematical framework and how this allows prediction (and therefore creativity) to proceed at levels of representation which are abstracted from the audible surface of music, potentially providing an account of how a composer or improviser may imagine larger scale musical structures. We propose tentative hypotheses about how the model might map on to a neural substrate.},
	language = {en},
	journal = {Journal of Cognition},
	author = {Homer, Steven T and Harley, Nicholas and Wiggins, Geraint A},
	year = {2023},
	note = {In press},
}

